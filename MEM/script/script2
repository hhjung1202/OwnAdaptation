python3 ../main.py --dir /home/hhjung/hhjung/NAE/ResNet18/Asym/n0.3/Ep10 --noise-rate 0.6 --noise-type Asym --epoch1 10 --epoch2 120 --epoch3 30 --gpu 1 1> /dev/null 2> /dev/null &&
python3 ../main.py --dir /home/hhjung/hhjung/NAE/ResNet18/Asym/n0.3/Ep20 --noise-rate 0.6 --noise-type Asym --epoch1 20 --epoch2 120 --epoch3 30 --gpu 1 1> /dev/null 2> /dev/null &&
python3 ../main.py --dir /home/hhjung/hhjung/NAE/ResNet18/Asym/n0.3/Ep30 --noise-rate 0.6 --noise-type Asym --epoch1 30 --epoch2 120 --epoch3 30 --gpu 1 1> /dev/null 2> /dev/null &&
python3 ../main.py --dir /home/hhjung/hhjung/NAE/ResNet18/Asym/n0.4/Ep10 --noise-rate 0.8 --noise-type Asym --epoch1 10 --epoch2 120 --epoch3 30 --gpu 1 1> /dev/null 2> /dev/null &&
