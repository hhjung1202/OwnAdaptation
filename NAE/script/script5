#!/bin/bash
python3 ../main.py --dir /data2/hhjung/NAE/noise2 --noise-rate 0.2 --gpu 4 1> /dev/null 2> noise2 &&
python3 ../main.py --dir /data2/hhjung/NAE/noise3 --noise-rate 0.3 --gpu 4 1> /dev/null 2> noise3 &&
python3 ../main.py --dir /data2/hhjung/NAE/noise4 --noise-rate 0.4 --gpu 4 1> /dev/null 2> noise4